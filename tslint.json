{
    "defaultSeverity": "error",
    "extends": ["tslint:recommended"],
    "rules": {
        "semicolon": false,
        "trailing-comma": false,
        "no-console": false,
        "interface-over-type-literal": false,
        "max-classes-per-file": false,
        "no-angle-bracket-type-assertion":true,
        "arrow-parens":false
    }
}
